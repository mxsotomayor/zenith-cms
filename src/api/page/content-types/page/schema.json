      "type": "string"
    },
    "body": {
      "type": "dynamiczone",
      "components": [
        "shared.rich-text",
        "shared.quote",
        "shared.media",
        "shared.hero-banner",
        "sliders.services-slider",
        "shared.hero-banner-item",
        "accordion.faq",
        "grids.four-blocks-grid",
        "grids.article-grid",
        "shared.alert-card",
        "shared.two-column-banner"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "metaTitle": {
      "type": "string"
    },
    "metaDescription": {
      "type": "string"
    },
    "sharedImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
